# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Notion API Configuration  
NOTION_API_KEY=your_notion_api_key_here
NOTION_DATABASE_ID=your_notion_database_id_here

# LLM Model Configuration (default: gpt-5-mini)
# Options: gpt-5-mini, gpt-5
LLM_MODEL=gpt-5-mini

# OpenAI Token Configuration
# Max completion tokens for responses (default: 4000)
# GPT-5 models support up to 128,000 reasoning & output tokens
OPENAI_MAX_COMPLETION_TOKENS=4000